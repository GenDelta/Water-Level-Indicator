void write_data(unsigned char Data)
{
  unsigned char b;
  for(b=0;b<8;b++){
    if (Data & 0x80)
    {
      digitalWrite(SDA_PIN,HIGH);;;;
    }
    else
    {
      digitalWrite(SDA_PIN,LOW);;;
    }
    Clock();
    Data <<= 1;       
  }
  pinMode(SDA_PIN, INPUT);
  Clock();
  delayMicroseconds(5);
  pinMode(SDA_PIN, OUTPUT);
}

void Clock(void)
{
  delayMicroseconds(1);
  digitalWrite(SCL_PIN,HIGH);   
  delayMicroseconds(5);
  digitalWrite(SCL_PIN,LOW);
  delayMicroseconds(1);
}

void startBit(void)
{
  digitalWrite(SDA_PIN,LOW);
  delayMicroseconds(5);
  digitalWrite(SCL_PIN,LOW);
}

void stopBit(void)
{
  digitalWrite(SCL_PIN,HIGH); 
  delayMicroseconds(5);
  digitalWrite(SDA_PIN,HIGH);
}

void write_cmd( unsigned char comm){   
  startBit();
  write_data(addr);
  write_data(0x00);
  write_data(comm);
  stopBit();
}